{
  "name": "sk-blockchain-helper",
  "displayName": "sk blockchain helper",
  "description": "helper functions for blockchain ",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:sk-blockchain-helper.copy-functions",
    "onCommand:sk-blockchain-helper.import-contracts"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sk-blockchain-helper.copy-functions",
        "title": "Sk - Copy Solidity Functions List"
      },
      {
        "command": "sk-blockchain-helper.import-contracts",
        "title": "Sk - Import Solidity Contracts from Clipboard"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}