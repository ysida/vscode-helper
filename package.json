{
  "name": "sk-vscode-helper",
  "publisher": "oliveoverflow",
  "displayName": "Blockchain Truffle Suite Helper",
  "description": "add suggestions to truffle suite javascript testing files by reading solidity contracts",
  "version": "0.0.8",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:sk-vscode-helper.paste-snippets-template",
		"onCommand:extension.helloWorld"
	],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sk-vscode-helper.paste-snippets-template",
        "title": "Sk - Paste Snippets Template"
      },
      {
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "fs-extra": "^10.1.0"
  }
}